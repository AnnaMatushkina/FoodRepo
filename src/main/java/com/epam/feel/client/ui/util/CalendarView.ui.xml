<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:with field='constants' type='com.epam.feel.client.EpamFeelConstants' />
	<ui:with field='st' type='com.epam.feel.client.EpamFeelResources' />
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<p1:DateBox ui:field="dateBox" />
			<g:Image ui:field="image" url="epam_feel/calendar.png" styleName="{st.global.calendarImage}" />
		</g:HorizontalPanel>
		<g:VerticalPanel>
			<g:Label ui:field="hint" styleName="{st.global.hint}" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 